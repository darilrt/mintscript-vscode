{
  "name": "mintscript",
  "displayName": "MintScript",
  "description": "Support for MintScript",
  "publisher": "mintscript",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "mintscript",
      "aliases": ["MintScript", "mintscript"],
      "extensions": [".mint"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "mintscript",
      "scopeName": "source.mint",
      "path": "./syntaxes/mintscript.tmLanguage.json"
    }]
  },
  "repository": "https://github.com/darilrt/mintscript-vscode.git"
}
